<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre %>
  <%= collection_select(:song, :genre, Genre.all, :name, :name) %>
  <br />
  <label>Note 1</label>
  <input name="song[notes][][:content]">
  <br />
  <label>Note 2</label>
  <input name="song[notes][][:content]">
  <br />
  <label>Note 3</label>
  <input name="song[notes][][:content]">
  <br />
  <%= f.submit %>
<% end %>


